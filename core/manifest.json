{
  "manifest_version": 3,
  "name": "ShieldEye",
  "version": "0.1.0",
  "description": "Advanced anti-bot and captcha detection system for web security analysis",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "webRequest",
    "webNavigation",
    "tabs",
    "scripting",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "ShieldEye V0.1",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/providers/*", 
        "detectors/**/*",
        "scripts/pbf.js", 
        "scripts/message.browser.js",
        "modules/*.js",
        "modules/capture/*.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}